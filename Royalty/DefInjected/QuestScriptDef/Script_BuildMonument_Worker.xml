<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: Monument completed -->
  <BuildMonumentWorker.LetterLabelMonumentConstructed.slateRef>モニュメント完成</BuildMonumentWorker.LetterLabelMonumentConstructed.slateRef>
